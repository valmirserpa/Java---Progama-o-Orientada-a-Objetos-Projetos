package entities;

public class Products {
	
	private String name;
	private int price;
	private double tax;
	
	
	public int somarValor(Double price) {
		this.price=price;
	}
	
	public double dobrarValor(double price) {
		this.price*=price;
	}
	
	
	public void setName(String name) {
		this.name=name;
	}
	

}
